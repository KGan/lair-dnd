
  <video class='banner-video video' autoplay loop>
    <source src='https://s3-us-west-2.amazonaws.com/lairdnd-prod/banner.webm' type='video/webm'>
    Your browser does not support the video tag.
  </video>

  <div class='welcome'>
    <div class='title'><span class='welcome_message'>Welcome Home</span></div>
    <div class='tag'><span class='tagline'>Rent Places to Start Your Conquest</span></div>
    <div class='col-md-12 how-it-works'>
      <button class='about btn btn-large'><span class='h4'>How It Works</span></div>
  </div>
  </div>
  <form class='form-field'>
    <div class='col-md-7 full-search-form search-form'>
      <div class="form-group col-md-4">
        <input placeholder="Where are you staying?" id='search-input' value="">
      </div>
      <div class='col-md-8 equipartition'>
        <div class="form-group col-md-3">
          <input name='search[checkin]' class="datepicker " id="checkin" placeholder="Check In">
        </div>
        <div class="form-group col-md-3">
          <input name='search[checkout]' class="datepicker " id="checkout" placeholder="Check Out">
        </div>
        <div class='form-group col-md-3'>
            <select name='search[guests]' class='form-control' id='guests'>
              <% for(var i = 1; i < 15; i++) { %>
                <option value='<%= i %>'>
                  <%= i %> Guest<% if (i > 1) { %>s<% } %>
                </option>
              <% } %>
            </select>
        </div>
        <div class="form-group col-md-3">
          <button class="submit search form-control">Submit</button>
        </div>
      </div>
    </div>
  </form>
