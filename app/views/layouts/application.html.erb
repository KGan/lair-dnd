<!DOCTYPE html>
<html>
<head>
  <title>LairDnD</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <script type="text/javascript" src="http://api.filepicker.io/v1/filepicker.js"></script>
</head>
<body>
  <div id='main'>
    <div id='navbar'>
      <%= render 'shared/navbar' %>
    </div>
    <div id='flashes'>
      <%= render 'shared/flashes' %>
    </div>
    <div id='main-content'>
      <%= yield %>
    </div>
  </div>


</body>
</html>
